<!-- begin page-header -->
<h1 class="page-header">Calendar <small>Coming Soon</small></h1>
<!-- end page-header -->
<!-- begin panel -->
<div class="panel panel-inverse">
  <div class="panel-heading">
    <div class="panel-heading-btn">
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
        <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
    </div>
    <h4 class="panel-title">Calendar</h4>
  </div>
  <div class="panel-body p-0">
    <div class="vertical-box">
      <div class="vertical-column p-15 bg-silver width-sm">
        <div id="external-events" class="calendar-event width-sm">
          <h4 class=" m-b-20">Events</h4>
          <div class="external-event bg-purple" data-bg="bg-purple" data-title="" data-media="<i class='fa fa-comments'></i>" data-desc="">
              
          </div>
          <div class="external-event bg-blue" data-bg="bg-blue" data-title="" data-media="<i class='fa fa-cutlery'></i>" data-desc=""> 
              
          </div>
          <div class="external-event bg-green" data-bg="bg-green" data-title="" data-media="<i class='fa fa-lightbulb-o'></i>" data-desc="">
              
          </div>
          <div class="external-event bg-orange" data-bg="bg-orange" data-title="" data-media="<i class='fa fa-tasks'></i>" data-desc="">
          </div>
          <div class="external-event bg-red" data-bg="bg-red" data-title="" data-media="<i class='fa fa-video-camera'></i>" data-desc="">
          </div>
          <div class="checkbox">
              <input type="checkbox" id="drop-remove" />
              <label for="drop-remove">remove after drop</label>
          </div>
        </div>
      </div>
      <div id="calendar" class="vertical-column p-15 calendar"></div>
    </div>
  </div>
</div>
<!-- end panel -->
</div>
<!-- end #content -->

